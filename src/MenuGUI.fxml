<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MenuGUIController">
   <children>
      <Label layoutX="43.0" layoutY="36.0" prefHeight="49.0" prefWidth="532.0" text="Welcome To Mivhanet System">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <Button fx:id="createQuestion_btn" layoutX="180.0" layoutY="133.0" mnemonicParsing="false" onAction="#createQuestion" prefHeight="39.0" prefWidth="240.0" text="Create Question">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="editQuestion_btn" layoutX="180.0" layoutY="263.0" mnemonicParsing="false" onAction="#editQuestion" prefHeight="31.0" prefWidth="242.0" text="Edit  Question">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="addCommentToQuestion_btn" layoutX="179.0" layoutY="198.0" mnemonicParsing="false" onAction="#addCommentToQuestion" text="Add Comment To Question">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
